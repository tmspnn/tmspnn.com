{
    "name": "tmspnn.com",
    "version": "1.0.0",
    "description": "Frontend & gateway of tmspnn.com",
    "main": "app/pages/index/index.js",
    "scripts": {
        "test": "ava",
        "fmt": "npx prettier --write app",
        "fmt-test": "npx prettier --write test",
        "build": "webpack --node-env=production",
        "dev": "webpack --watch"
    },
    "dependencies": {
        "@editorjs/code": "^2.7.0",
        "@editorjs/delimiter": "^1.2.0",
        "@editorjs/editorjs": "^2.22.1",
        "@editorjs/header": "^2.6.1",
        "@editorjs/image": "^2.6.0",
        "@editorjs/inline-code": "^1.3.1",
        "@editorjs/list": "^1.6.2",
        "@editorjs/quote": "^2.4.0",
        "big.js": "^6.1.1",
        "dayjs": "^1.10.6",
        "editorjs-text-color-plugin": "^1.1.22",
        "highlight.js": "^11.1.0",
        "js-base64": "^3.6.1",
        "k-dom": "^0.3.2",
        "k-util": "^0.3.16",
        "k-xhr": "^0.3.4",
        "lodash": "^4.17.21",
        "normalize.css": "^8.0.1",
        "qs": "^6.10.1",
        "spin.js": "^4.1.0",
        "validator": "^13.6.0",
        "video.js": "^7.13.3"
    },
    "devDependencies": {
        "ava": "^3.15.0",
        "css-loader": "^6.0.0",
        "html-loader": "^2.1.2",
        "mini-css-extract-plugin": "^2.1.0",
        "sass": "^1.35.2",
        "sass-loader": "^12.1.0",
        "webpack": "^5.44.0",
        "webpack-cli": "^4.7.2"
    },
    "ava": {
        "files": [
            "test/**/*"
        ],
        "concurrency": 4,
        "failFast": true
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "quoteProps": "consistent",
        "jsxSingleQuote": false,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "arrowParens": "always"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tmspnn/tmspnn.com"
    },
    "keywords": [
        "tmspnn",
        "openresty",
        "lua",
        "rest"
    ],
    "author": "Thomas Peng Li",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tmspnn/tmspnn.com/issues"
    },
    "homepage": "https://github.com/tmspnn/tmspnn.com#readme"
}
