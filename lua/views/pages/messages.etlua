<%
local title = ctx.page_title
local messages = ctx.data.messages or {}
%>
<div class="page-container">
    <div class="personal">
        <% for i, m in ipairs(messages.personal) do %>
        <% render("views/components/message", m) %>
        <% end %>
    </div>
    <div class="organizations"></div>
</div>
<% render("views/components/navbar", { title = title }) %>
<% render("views/components/tabbar", { active_tab = "messages" }) %>